{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "pnpm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "gql-client-module": {
      "projectType": "library",
      "root": "projects/gql-client-module",
      "sourceRoot": "projects/gql-client-module/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/gql-client-module/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/gql-client-module/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/gql-client-module/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/gql-client-module/src/test.ts",
            "tsConfig": "projects/gql-client-module/tsconfig.spec.json",
            "karmaConfig": "projects/gql-client-module/karma.conf.js"
          }
        }
      }
    },
    "app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "less"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/app",
      "sourceRoot": "projects/app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app",
            "index": "projects/app/src/index.html",
            "main": "projects/app/src/main.ts",
            "polyfills": "projects/app/src/polyfills.ts",
            "tsConfig": "projects/app/tsconfig.app.json",
            "inlineStyleLanguage": "less",
            "assets": [
              "projects/app/src/favicon.ico",
              "projects/app/src/assets"
            ],
            "styles": [
              "projects/app/src/styles.less"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/app/src/environments/environment.ts",
                  "with": "projects/app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "app:build:production"
            },
            "development": {
              "browserTarget": "app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/app/src/test.ts",
            "polyfills": "projects/app/src/polyfills.ts",
            "tsConfig": "projects/app/tsconfig.spec.json",
            "karmaConfig": "projects/app/karma.conf.js",
            "inlineStyleLanguage": "less",
            "assets": [
              "projects/app/src/favicon.ico",
              "projects/app/src/assets"
            ],
            "styles": [
              "projects/app/src/styles.less"
            ],
            "scripts": []
          }
        }
      }
    },
    "gql-api": {
      "projectType": "library",
      "root": "projects/gql-api",
      "sourceRoot": "projects/gql-api/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/gql-api/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/gql-api/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/gql-api/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/gql-api/src/test.ts",
            "tsConfig": "projects/gql-api/tsconfig.spec.json",
            "karmaConfig": "projects/gql-api/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "gql-client-module"
}
